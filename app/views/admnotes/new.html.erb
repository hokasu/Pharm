<h1>New admission note</h1>


<% form_for [@admission, @admnote] do |f| %>
  <%= f.error_messages %>

<p>
  <b>Author:</b>
  <%=h @admnote.username %>
  <%= f.text_field :username %>
</p>
  <p>
    <%= f.label :text %><br />
    <%= f.text_area :text %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Admission notes', admission_admnotes_path (@admission) %> |
