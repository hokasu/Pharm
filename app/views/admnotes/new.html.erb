<% title("New admission note") %>


<% form_for [@admission, @admnote] do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :text %><br />
    <%= f.text_area :text %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Admission notes', admission_admnotes_path (@admission) %> |
