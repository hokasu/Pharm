<h1>Editing admission note</h1>

<p>
  <b>Created:</b>
  <%=h @admnote.created_at %>
  <% if updated? %>
    <b>Updated:</b>
    <%=h @admnote.updated_at %>
  <% end %>
</p>
<p>
  <b>Author:</b>
  <%=h @admnote.username %>
</p>

<% form_for(@admnote) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :text %><br />
    <%= f.text_area :text %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Admission Notes', admnotes_path %> 
