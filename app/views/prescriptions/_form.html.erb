<% form_for @prescription do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :start %><br />
    <%= f.datetime_select :start %>
  </p>
  <p>
    <%= f.label :end %><br />
    <%= f.datetime_select :end %>
  </p>
  <p>
    <%= f.label :ancillary_instructions %><br />
    <%= f.text_area :ancillary_instructions %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
