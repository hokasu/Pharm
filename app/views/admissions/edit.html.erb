<h1>Editing admission</h1>

<% form_for(@admission) do |f| %>
  <%= f.error_messages %>

<%= render :partial => 'form', :locals => {:f => f} %>
   <p>
    <%= f.submit 'Update' %>
    </p>

<% end %>

<%= link_to 'New note', new_admission_admnote_path (@admission) %> |
<%= link_to 'Notes', admission_admnotes_path (@admission) %> |
<%= link_to 'Destroy', @admission, :confirm => 'Are you sure?', :method => :delete %> |
<%= link_to 'Cancel', admission_path  %>
